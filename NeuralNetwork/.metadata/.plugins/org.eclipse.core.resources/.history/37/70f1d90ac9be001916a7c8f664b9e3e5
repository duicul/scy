import java.util.List;

public class InputLayer extends Layer {
	private int size;
	public InputLayer(int size) {
		super(size);
		this.size=size;
	}

	@Override
	public List<Double> apply(List<Double> inp) {
		return inp;
	}
	
	public int size() {
		return this.size;}
	
	public String toString() {
		return size+" Inputs";}

	@Override
	public Double train(List<Double> inp, List<Double> target) {
		// TODO Auto-generated method stub
		return null;
	}

}
